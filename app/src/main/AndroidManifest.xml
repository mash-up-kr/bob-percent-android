<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mash.pig.bobpercent">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".BobPercentApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".app.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".app.sign.SignUpActivity"/>
        <activity android:name=".app.sign.LoginActivity"/>
        <activity android:name=".app.sign.PendingActivity"/>
        <activity android:name=".app.sign.InputCodeActivity"/>
        <activity android:name=".app.FoodOrHisActivity"/>
        <activity android:name=".app.sign.GenCodeActivity"/>
        <activity android:name=".app.HistoryActivity"/>
        <activity android:name=".app.AnalysisActivity"/>


    </application>

</manifest>